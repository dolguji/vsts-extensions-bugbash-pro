{
    "name": "bugbash_v2",
    "license": "MIT",
    "description": "Easier way to manage bug bashes",
    "private": true,
    "main": "webpack.config.js",
    "version": "1.0.0",
    "scripts": {
        "clean": "rimraf dist *.vsix",
        "clean:all": "npm run clean && rimraf typings node_modules",
        "build": "npm run clean && mkdir dist && webpack --display-error-details --progress --colors --output-path ./dist -p",
        "start": "webpack-dev-server -d --hot --progress --colors --https --content-base ./ --port 8888",
        "package:dev": "node ./configs/packageDev",
        "package:release": "npm run build && node ./configs/packageRelease",
        "postinstall": "typings install"
    },
    "devDependencies": {
        "@types/react": "15.0.35",
        "@types/react-dom": "15.5.1",
        "@types/jquery": "2.0.41",
        "webpack": "3.1.0",
        "webpack-dev-server": "2.5.1",
        "rimraf": "2.6.1",
        "sass-loader": "6.0.6",
        "style-loader": "0.18.2",
        "css-loader": "0.28.4",
        "ts-loader": "2.2.2",
        "file-loader": "0.11.1",
        "url-loader": "0.5.8",
        "node-sass": "4.5.3",
        "typescript": "2.3.4",
        "typings": "2.1.1",
        "uglifyjs-webpack-plugin": "0.4.6",
        "copy-webpack-plugin": "4.0.1",
        "tfx-cli": "0.4.9"
    },
    "dependencies": {
        "requirejs": "2.3.3",
        "vss-web-extension-sdk": "2.117.0",
        "es6-promise": "4.1.1",
        "office-ui-fabric-react": "4.16.2",
        "vsts-extension-react-widgets": "0.0.36",
        "trumbowyg": "2.5.1",
        "recharts": "0.22.4",
        "rc-split-layout": "1.0.0"
    }
}
